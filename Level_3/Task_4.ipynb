    "Task 4: Generator Sequence"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "716c8b1b-e48a-454e-ac96-910636614808",
   "metadata": {},
   "outputs": [],
   "source": [
    "def fib(n):\n",
    "    a, b = 0, 1\n",
    "    for _ in range(n):\n",
    "        yield a\n",
    "        a, b = b, a + b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "c3230ecc-e3cd-4b64-9bcb-6ffd64cbe556",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[0, 1, 1, 2, 3]\n"
     ]
    }
   ],
   "source": [
    "print(list(fib(5)))"
   ]
